{
  "name": "nestjs-cli-baseline",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node ./src/main.ts",
    "start:debug": "nodemon",
    "start:prod": "npm run db:migration:run && node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "DOTENV_CONFIG_PATH=./env.test jest",
    "test:migration": "dotenv -e env.test -- typeorm migration:run",
    "test:watch": "DOTENV_CONFIG_PATH=./env.test jest --watch",
    "test:cov": "DOTENV_CONFIG_PATH=./env.test jest --coverage",
    "test:debug": "DOTENV_CONFIG_PATH=./env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test:migration && jest --config ./test/jest-e2e.json -runInBand",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -d ./ormconfig.ts",
    "db:drop": "npm run typeorm:cli schema:drop",
    "db:migration:generate": "typeorm migration:create",
    "db:create": "ts-node src/scripts/create-database.ts",
    "db:migration:revert": "npm run typeorm:cli migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync",
    "db:migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d ./ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sinon": "^14.0.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.9",
    "url-join-ts": "^1.0.5",
    "uuid": "^9.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.0.0",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.6",
    "chai-spies": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
